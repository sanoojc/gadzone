<div class="container-fluid pb-5">
    <div class="row px-xl-5">
        <div class="col-lg-5 mb-30 d-flex justify-content-center flex-column">
            <div class="show" href="/uploads/{{product.mainImage.filename}}">
                <img src="/uploads/{{product.mainImage.filename}}" id="show-img" style="object-fit:contain">
            </div>

            <!-- Secondary carousel image thumbnail gallery -->

            <div class="small-img">
                <img src="/img/next-icon.png" class="icon-left" alt="" id="prev-img">
                <div class="small-container">
                    <div id="small-img-roll">
                        {{#each product.sideImages}}
                        <img src="/uploads/{{this.filename}}" class="show-small-img" style="object-fit:contain">
                        {{/each}}
                    </div>
                </div>
                <img src="/img/next-icon.png" class="icon-right" alt="" id="next-img">
            </div>

        </div>

        <div class="col-lg-7 h-auto mb-30 ">
            <div class="h-100 bg-light p-30">
                <h3>{{product.productName}}
                    {{!-- <i class="fas fa-heart text-primary"></i> --}}
                </h3>
                <div class="d-flex mb-3">
                    <div class="text-primary mr-2">
                        <small class="fas fa-star"></small>
                        <small class="fas fa-star"></small>
                        <small class="fas fa-star"></small>
                        <small class="fas fa-star-half-alt"></small>
                        <small class="far fa-star"></small>
                    </div>
                    <small class="pt-1">(99 Reviews)</small>
                </div>
                <h3 class="font-weight-semi-bold mb-4">{{product.productPrice}}</h3>
                <p class="mb-4">{{product.description}}</p>
                <div class="d-flex justify-content-around w-50 mb-4 pt-2">


                    {{!-- <button class="btn btn-primary px-3 " onclick="location.href='/addToCart/{{product._id}}'">Add
                        To Whishlist</button> --}}
                    <button class="btn btn-primary  px-3 " onclick="location.href='/addToCart/{{product._id}}'"><i
                            class="fa fa-shopping-cart mr-1"></i> Add To
                        Cart</button>
                    <button class="btn btn-primary m  px-3 " onclick="location.href='/addToWishlist/{{product._id}}'">
                  Add To Wishlist</button>
                </div>
                {{!-- <button class="btn btn-primary  px-3 " onclick="location.href='/checkout/{{product._id}}'">Buy
                    Now</button> --}}
            </div>
        </div>
