    <div class="container">
        <div class="main-body">
            <div class="row gutters-sm">
                <div class="col-md-4 mb-3">
                    <div class="card">
                        <div class="card-body">
                            <div class="d-flex flex-column align-items-center text-center">
                                <img src="https://bootdey.com/img/Content/avatar/avatar7.png" alt="Admin"
                                    class="rounded-circle" width="150">
                                <div class="mt-3">
                                    <h4 class="text-dark">{{user.name}}</h4>
                                    <p class="text-dark mb-1">{{user.email}}</p>
                                    <button class="btn btn-edit-btn">Edit</button>
                                </div>
                            </div>
                        </div>
                    </div>



                    <div class="card mt-3">
                        <ul class="list-group list-group-flush text-center">
                            <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                <a href="/cart"><h6 class="btn mb-0">cart</h6></a> 
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                <a href=""><h6 class="btn mb-0">wishlist</h6></a> 
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                               <a href="/orders"><h6 class="btn mb-0">orders</h6></a> 
                            </li>
                            <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                                 <a  href="/addAddress"><h6 class="btn mb-0">Add Adress</h6></a> 
                            </li>  

                        </ul>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="row gutters-sm">
                        {{#each user.adress}}
                        <div class="col-sm-6 mb-3">
                            <div class="card h-100">

                                <div class="card-body text-dark">
                                    <h6 class="d-flex align-items-center mb-3"> Adress</h6>
                                    <p>Name : {{this.name}}</p>
                                    <p>Address : {{this.adress}}</p>
                                    <p>City : {{this.city}}</p>
                                    <p>State : {{this.state}}</p>
                                    <p>Pin : {{this.pin}}</p>
                                    <p>Number : {{this.mobile}}</p>
                                    <div class="col-sm-12">
                                        <a class="btn btn-info " target="__blank" href="/editAddress/{{this.id}}">Edit </a>
                                        <a class="btn deletebtn btn-info " target="__blank" href="/deleteAddress/{{this.id}}" onclick="return confirm('are you sure want to delete')">Delete </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>



                </div>
